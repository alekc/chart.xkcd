  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-private-methods": "^7.4.4",
    "gh-pages": "^2.1.1"
  },
  "dependencies": {
    "d3": "^7.6.1"
  }
}
