{
  "name": "chart.xkcd",
  "version": "1.0.1",
  "description": "xkcd style chart lib",
  "source": "src/chart.xkcd.js",
  "main": "dist/chart.xkcd.js",
  "module": "dist/chart.xkcd.mjs",
  "unpkg": "dist/chart.xkcd.umd.js",
  "scripts": {
    "build": "rm -r dist && parcel build src/chart.xkcd.js --global chartXkcd",
